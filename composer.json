        "hoa/visitor": "<1.14.12.10",
        "hoa/compiler": "<2.14.12.10",
        "hoa/file": "<0.14.12.10",
        "hoa/stream": "<0.14.12.10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Gallou",
            "email": "adriengallou@gmail.com"
        }
